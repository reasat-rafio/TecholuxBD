n         const { data } = await axios.post(`/api/auth/login`, {\n            email,\n            password,\n         });\n         searchDispatch(globalLoadingEndAction());\n         userDispatch(loginUserAction(data.data.newUser));\n         router.push(\"/\");\n         snackbarDispatch(\n            setSnackbar(true, \"success\", \"Logged in successfully!\")\n         );\n      } catch (error) {\n         snackbarDispatch(\n            setSnackbar(true, \"error\", `${error.response.data.error}`)\n         );\n      }\n   };\n\n   return (\n      <form className=\"sign-in-form\" onSubmit={handleSubmit(onSignInSubmit)}>\n         <h2 className=\"title\">Sign in</h2>\n         <TextField\n            className=\"textField\"\n            variant=\"outlined\"\n            margin=\"normal\"\n            fullWidth\n            label=\"Email Address\"\n            name=\"email\"\n            inputRef={register}\n            autoFocus\n         />\n         {errors.email && (\n            <Typography variant=\"body2\" color=\"secondary\">\n               {errors.email.message}\n            </Typography>\n         )}\n         <TextField\n            className=\"textField\"\n            variant=\"outlined\"\n            margin=\"normal\"\n            inputRef={register}\n            fullWidth\n            label=\"Password\"\n            name=\"password\"\n         />\n         {errors.password && (\n            <Typography variant=\"body2\" color=\"secondary\">\n               {errors.password.message}\n            </Typography>\n         )}\n         <input type=\"submit\" value=\"Login\" className=\"btn solid\" />\n         <div>\n            <Typography variant=\"caption\" className={classes.h2}>\n               <span className={classes.span}>\n                  Or Sign in with social platforms\n               </span>\n            </Typography>\n         </div>\n         <div className={classes.socials}>\n            <Button\n               variant=\"outlined\"\n               className={classes.iconBtn}\n               onClick={() => socialAuthenticationAction(\"google\")}\n            >\n               <span className={classes.icon}>\n                  <Image\n                     src={GoogleIcon}\n                     height=\"40\"\n                     width=\"40\"\n                     layout=\"intrinsic\"\n                  />\n               </span>\n            </Button>\n\n            <Button\n               variant=\"outlined\"\n               className={classes.iconBtn}\n               onClick={() => socialAuthenticationAction(\"facebook\")}\n            >\n               <span className={classes.icon}>\n                  <Image\n                     src={FacebookIcon}\n                     height=\"40\"\n                     width=\"40\"\n                     layout=\"intrinsic\"\n                  />\n               </span>\n            </Button>\n\n            <Button\n               variant=\"outlined\"\n               className={classes.iconBtn}\n               onClick={() => socialAuthenticationAction(\"github\")}\n            >\n               <span className={classes.icon}>\n                  <Image\n                     src={GuthubIcon}\n                     height=\"40\"\n                     width=\"40\"\n                     layout=\"intrinsic\"\n                  />\n               </span>\n            </Button>\n         </div>\n      </form>\n   );\n};\n"]},"metadata":{},"sourceType":"module"}